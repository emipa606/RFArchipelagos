<?xml version="1.0" encoding="utf-8"?>
<Patch>
  <Operation Class="PatchOperationConditional">
    <xpath>/Defs/VCE_Fishing.BiomeTempDef[defName="VCEF_BiomeCold"]</xpath>
    <success>Always</success>
    <match Class="PatchOperationAdd">
      <xpath>/Defs/VCE_Fishing.BiomeTempDef[defName="VCEF_BiomeCold"]/biomes</xpath>
      <value>
        <li>BorealArchipelago</li>
        <li>BorealArchipelago_Fresh</li>
        <li>TundraArchipelago</li>
        <li>TundraArchipelago_Fresh</li>
        <li>ColdBogArchipelago</li>
        <li>ColdBogArchipelago_Fresh</li>
      </value>
    </match>
  </Operation>
  <Operation Class="PatchOperationConditional">
    <xpath>/Defs/VCE_Fishing.BiomeTempDef[defName="VCEF_BiomeWarm"]</xpath>
    <success>Always</success>
    <match Class="PatchOperationAdd">
      <xpath>/Defs/VCE_Fishing.BiomeTempDef[defName="VCEF_BiomeWarm"]/biomes</xpath>
      <value>
        <li>TemperateArchipelago</li>
        <li>TemperateArchipelago_Fresh</li>
        <li>TemperateSwampArchipelago</li>
        <li>TemperateSwampArchipelago_Fresh</li>
        <li>TropicalArchipelago</li>
        <li>TropicalArchipelago_Fresh</li>
        <li>TropicalSwampArchipelago</li>
        <li>TropicalSwampArchipelago_Fresh</li>
      </value>
    </match>
  </Operation>
  <Operation Class="PatchOperationConditional">
    <xpath>/Defs/VCE_Fishing.BiomeTempDef[defName="VCEF_BiomeHot"]</xpath>
    <success>Always</success>
    <match Class="PatchOperationAdd">
      <xpath>/Defs/VCE_Fishing.BiomeTempDef[defName="VCEF_BiomeHot"]/biomes</xpath>
      <value>
        <li>DesertArchipelago</li>
        <li>DesertArchipelago_Fresh</li>
      </value>
    </match>
  </Operation>
</Patch>
